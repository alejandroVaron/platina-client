{
  "name": "platina-client",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint './src/**/*.{js,ts,svelte}'",
    "format": "prettier --write './src/**/*.{js,ts,svelte,json,css,scss,md}'",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rimraf .svelte-kit build node_modules/.vite",
    "dev:prod": "cross-env NODE_ENV=production vite dev",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.7",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-operations": "^4.6.1",
    "@lucide/svelte": "^0.525.0",
    "@melt-ui/pp": "^0.3.2",
    "@sveltejs/adapter-auto": "^6.0.1",
    "@sveltejs/kit": "^2.21.0",
    "@sveltejs/vite-plugin-svelte": "^6.0.0",
    "@types/node": "^24.0.15",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "dotenv": "^17.2.1",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tailwind-merge": "^3.3.1",
    "tailwind-variants": "^1.0.0",
    "tailwindcss": "^4.1.11",
    "tw-animate-css": "^1.3.5",
    "typescript": "^5.0.0",
    "vaul-svelte": "1.0.0-next.7",
    "vite": "^7.0.4"
  },
  "overrides": {
    "cookie": "^0.7.0"
  },
  "dependencies": {
    "@melt-ui/svelte": "^0.86.6",
    "@tailwindcss/vite": "^4.1.11",
    "@tsparticles/slim": "^3.8.1",
    "@tsparticles/svelte": "^3.1.1",
    "@urql/svelte": "^4.2.3",
    "bits-ui": "2.8.11",
    "clsx": "^2.1.1",
    "dotenv-flow": "^4.1.0",
    "graphql": "^16.11.0",
    "lucide-svelte": "^0.525.0",
    "melt": "^0.38.0",
    "svelte-virtual": "^1.0.0-next.22"
  }
}
