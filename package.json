{
  "name": "platina-client",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint './src/**/*.{js,ts,svelte}'",
    "format": "prettier --write './src/**/*.{js,ts,svelte,json,css,scss,md}'",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rimraf .svelte-kit build node_modules/.vite",
    "dev:prod": "cross-env NODE_ENV=production vite dev"
  },
  "devDependencies": {
    "@melt-ui/pp": "^0.3.2",
    "@sveltejs/adapter-auto": "^6.0.1",
    "@sveltejs/kit": "^2.21.0",
    "@sveltejs/vite-plugin-svelte": "^6.0.0",
    "@types/node": "^24.0.15",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.0.0",
    "vite": "^7.0.4"
  },
  "overrides": {
    "cookie": "^0.7.0"
  },
  "dependencies": {
    "@melt-ui/svelte": "^0.86.6",
    "@tailwindcss/vite": "^4.1.11",
    "clsx": "^2.1.1",
    "dotenv-flow": "^4.1.0",
    "lucide-svelte": "^0.525.0",
    "melt": "^0.38.0"
  }
}
